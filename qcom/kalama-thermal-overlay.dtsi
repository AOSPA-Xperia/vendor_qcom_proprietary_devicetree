	};

	pm8550vs_e_tz {
		cooling-maps {
			pm8550vs_e_nsp {
				trip = <&pm8550vs_e_trip0>;
				cooling-device = <&cdsp_sw 5 THERMAL_NO_LIMIT>;
			};
		};
