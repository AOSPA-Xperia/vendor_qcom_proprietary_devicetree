#include <dt-bindings/thermal/thermal_qti.h>

&thermal_zones {
	socd {
		cooling-maps {
			socd_apc1 {
				trip = <&socd_trip>;
				cooling-device = <&APC1_pause 1 1>;
			};

			socd_cdsp1 {
				trip = <&socd_trip>;
				cooling-device = <&cdsp_sw 4 4>;
			};
		};
	};

	pm8550b-bcl-lvl0 {
		cooling-maps {
			lbat_cpu_3_4_5 {
				trip = <&b_bcl_lvl0>;
				cooling-device = <&cpu_3_4_5_pause 1 1>;
			};

			lbat_cdsp0 {
				trip = <&b_bcl_lvl0>;
				cooling-device = <&cdsp_sw 2 2>;
			};
		};
	};

	pm8550b-bcl-lvl1 {
